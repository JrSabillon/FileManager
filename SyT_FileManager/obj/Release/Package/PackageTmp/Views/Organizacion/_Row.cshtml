@model SyT_FileManager.Models.EstructuraOrganizacionalModel

<tr>
    <td>@Model.EstOrgaID</td>
    <td>@Model.EstOrgaNombre</td>
    <td>@Model.EstOrgaAbreviatura</td>
    <td>@Model.EstOrgaStatus</td>
    <td class="text-right">
        <button class="btn btn-sm btn-success" type="button" onclick="organizationAction(0, @Model.EstOrgaID)"><i class="fa fa-plus"></i></button>
        <button class="btn btn-sm btn-warning" type="button" onclick="organizationAction(@Model.EstOrgaID, @Model.EstOrgaIDPadre)"><i class="fa fa-pencil"></i></button>
        @if (Model.Estructuras.Count > 0)
        {
            <button class="btn btn-sm btn-enterprise" data-toggle="collapse" data-target="#item_@Model.EstOrgaID">
                <i class="fa fa-chevron-down"></i>
            </button>
        }
    </td>
</tr>
