<h4>
    Rol: @(((SyT_FileManager.Models.RoleModel)ViewBag.Rol).RolNombre)
</h4>
<hr />
<div class="row" style="max-height: 500px; overflow-y: auto;">
    <div class="col-12">
        <h4>Privilegios del rol</h4>
        <ul>
            @foreach(SyT_FileManager.Models.PrivilegioModel item in ViewBag.Privilegios)
            {
                <li>@item.PrivNombre - @item.PrivDescripcion</li>
            }
        </ul>
    </div>
</div>
